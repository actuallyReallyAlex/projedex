{
  "name": "projedex",
  "version": "0.0.0",
  "description": "Like a pok√©dex, but for your projects.",
  "scripts": {
    "start": "npm run build && node src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "webpack --mode production",
    "build-local": "env-cmd -e development webpack --mode production",
    "client": "env-cmd -e development webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "env-cmd -e development nodemon src/server/index.js",
    "server-debug": "env-cmd -e development node --inspect src/server/index.js",
    "format": "prettier --write \"**/*.{js,json,md,html}\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@octokit/rest": "^16.43.1",
    "@sentry/browser": "^5.12.1",
