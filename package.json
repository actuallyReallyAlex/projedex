    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
